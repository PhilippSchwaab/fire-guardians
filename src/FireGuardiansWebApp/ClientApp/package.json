{
  "name": "fireguardians",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44486 --host=0.0.0.0 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44486 --host=0.0.0.0 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "node src/environments/version.ts",
    "codegen": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config codegen.yml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.18",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/material": "^19.2.18",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@angular/service-worker": "^19.2.14",
    "@angular/google-maps": "^19.2.18",
    "@meshmakers/octo-services": "^3.2.85-0",
    "@meshmakers/octo-ui": "^3.2.85-0",
    "@meshmakers/shared-auth": "^3.2.85-0",
    "@meshmakers/shared-services": "^3.2.85-0",
    "@meshmakers/shared-ui": "^3.2.85-0",
    "angular-oauth2-oidc": "^19.0.0",
    "apollo-angular": "12.1.0",
    "aspnet-prerendering": "^3.0.1",
    "date-fns": "^4.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "material-symbols": "^0.38.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^19.2.14",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^22.15.30",
    "cross-env": "^10.0.0",
    "jasmine-core": "~5.12.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.9.0"
  }
}
